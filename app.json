{
  "expo": {
    "name": "Face Off",
    "slug": "Face-Off",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "assets\\splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#bbf1a2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist":{
        "NSFaceIDUsageDescription": "this app uses the face authenticator to sign in"
      }
    }
  }
}
